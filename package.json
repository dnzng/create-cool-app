  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnzng/create-cool-app.git"
  },
  "bugs": {
    "url": "https://github.com/dnzng/create-cool-app/issues"
  },
  "homepage": "https://github.com/dnzng/create-cool-app#readme",
