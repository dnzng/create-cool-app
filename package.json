{
  "name": "create-cool-app",
  "version": "1.0.12",
  "description": "Generate automatically some common templates",
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": ">=16"
  },
  "keywords": [
    "template"
  ],
  "author": "Dylan Zhang",
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "create-cool-app": "index.js",
    "cca": "index.js"
  },
  "files": [
    "index.js",
    "template-*/**"
  ],
  "publishConfig": {
