{
  "name": "--projectname--",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "author": "--username--",
  "license": "MIT",
  "homepage": "https://github.com/dnzng/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnzng/utils.git"
  },
  "bugs": {
    "url": "https://github.com/dnzng/utils/issues"
  },
  "keywords": [],
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "bumpp --commit --tag --push && npm publish",
    "test": "vitest",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@dnzng/eslint-config-basic": "^0.1.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "bumpp": "^9.1.1",
    "esbuild": "^0.18.17",
    "eslint": "^8.45.0",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-esbuild": "^5.0.0",
    "simple-git-hooks": "^2.9.0",
    "vitest": "^0.33.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "eslint . --fix"
    ]
  }
}
